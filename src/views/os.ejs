<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>lol os</title>
    <%- include('others/head.ejs') %>
    <link rel="stylesheet" href="/css/others/nav.css">
    <link rel="stylesheet" href="/css/others/rc.css">
    <% apps.forEach(app => {%>
        <link rel="stylesheet" href="/css/apps/<%=app._id%>.css">
    <%})%>
    <script>
        var userID = `<%-userID%>`
    </script>
    <script defer src="http://localhost:3404/socket.io/socket.io.js"></script>
    <script defer src="/js/socket.js"></script>
</head>
<body onload="LoadSession()">
    <% apps.forEach(app => {%>
        <%- include('apps/' + app._id+'.ejs') %>
    <%})%>
    <%- include('others/rc.ejs') %> 
    <%- include('apps/desktop.ejs') %>
    <%- include('others/nav.ejs') %>
    <script>
    var session = <%- session%>;
    document.getElementsByTagName("body")[0].style = "background: url(<%=os.background%>); background-repeat: no-repeat; background-size: cover;" 
    </script>
    <script src="/js/apps.js"></script>
</body>
</html>